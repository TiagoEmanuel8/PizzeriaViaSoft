generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Order {
  id             String   @id @default(auto()) @map("_id") @db.ObjectId
  size           String
  flavor         String
  customizations String[]
  amount         Float
  time           Float
}
